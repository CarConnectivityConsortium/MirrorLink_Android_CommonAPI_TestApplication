<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        
    <Button
        android:id="@+id/playpauseButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Play" />
    
    <Button
        android:id="@+id/stopButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stop" />
    
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/rewindButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rewind" />
	
	    <Button
	        android:id="@+id/previousButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="Previous" />

	    <Button
	        android:id="@+id/nextButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="Next" />

	    <Button
	        android:id="@+id/fwdButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="Forward" />
        
    </LinearLayout>

  



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
	    
        <Button
            android:id="@+id/muteButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mute" />

        <Button
            android:id="@+id/photoButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Photo" />
        <Button
            android:id="@+id/zoomInButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ZoomIn" />
        <Button
            android:id="@+id/zoomOutButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ZoomOut" />
        
    </LinearLayout>
    
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:columnCount="4">
        <Button
	        android:id="@+id/f1Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="F1" />

	   <Button
	       android:id="@+id/f2Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F2" />
	   <Button
	       android:id="@+id/f3Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F3" />
	
	   <Button
	       android:id="@+id/f4Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F4" />
	
	   <Button
	       android:id="@+id/f5Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F5" />
	
	   <Button
	       android:id="@+id/f6Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F6" />
	
	   <Button
	       android:id="@+id/f7Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F7" />
	
	   <Button
	       android:id="@+id/f8Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F8" />
	
	   <Button
	       android:id="@+id/f9Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F9" />
	
	
	   <Button
	       android:id="@+id/f10Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F10" />
	
	   <Button
	       android:id="@+id/f11Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F11" />
	
	   <Button
	       android:id="@+id/f12Button"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:text="F12" />

   </GridLayout>

    <TextView
        android:id="@+id/multimediaLogs"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:singleLine="false" />

</LinearLayout>